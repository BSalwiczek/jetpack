  "healthier": {
    "ignore": [
      "examples",
      "test/fixtures/pkg-with-everything"
    ],
    "global": [
      "describe",
      "it",
      "afterEach",
      "beforeEach"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "timeout": "1m"
  }
