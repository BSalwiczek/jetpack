    "express": "^4.18.2",
    "fs-extra": "^11.1.0",
    "handlebars": "^4.7.7",
    "inquirer-confirm": "^2.0.7",
    "mini-css-extract-plugin": "^2.7.2",
    "parseurl": "^1.3.3",
